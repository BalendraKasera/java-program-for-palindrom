ublic class BinarytoDec {
    public static void bintodec(int binNum){
        int myNum=binNum;
             int pow=0;
             int deci=0;
             while(binNum>0){
                        int ld=binNum%10;
                        deci=deci+(ld*(int)Math.pow(2 , pow));
                        binNum=binNum/10;
                            pow++;
                            

             }
                  System.out.println("decimal of " +  myNum +" = " + deci );
            }
             public static void main(String args[]){

                            bintodec(101010 ); // change binary accordingly
             }

